{
	"require": {
		"guzzlehttp/guzzle": "^7.8",
		"datatables/datatables": "^1.10",
		"twbs/bootstrap": "^5.3",
		"guzzlehttp/psr7": "^2.6",
		"http-interop/http-factory-guzzle": "^1.2",
		"auth0/auth0-php": "^8.11",
		"vlucas/phpdotenv": "^5.6"

	},
	"autoload": {
			"psr-4": {
				"Controllers\\": "Controllers/",
